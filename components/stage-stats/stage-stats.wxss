/* 组合弹窗 */
.combine {
  width: 602rpx;
  background: #ffffff;
  border-radius: 10px;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 10;
  padding: 0 40rpx;
  padding-bottom: 40rpx;
}

.combine-close {
  width: 36rpx;
  height: 36rpx;
  text-align: center;
  line-height: 32rpx;
  color: #fff;
  background: #a4a4a4;
  border-radius: 50%;
  position: absolute;
  top: 33rpx;
  right: 33rpx;
}

.combine-delete {
  font-size: 24rpx;
  margin-left: 3rpx;
}

.combine-biti {
  font-size: 32rpx;
  font-weight: bold;
  color: #393939;
  margin-top: 52rpx;
}

.combine-title {
  font-size: 28rpx;
  font-weight: bold;
  color: #393939;
  margin-top: 16rpx;
}

.subtitle {
  margin-top: 8rpx;
}

.combine-remarks {
  font-size: 30rpx;
  font-weight: 400;
  line-height: 40rpx;
  color: #7e7e7e;
  margin-top: 4rpx;
}

.combine-mung {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.38;
}

.combine-but {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1rpx solid #ececec;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
  color: #738297;
  position: absolute;
  bottom: 0;
  left: 0;
}

.comese-list {
  display: flex;
}

.color-piece {
  width: 24rpx;
  height: 24rpx;
  border-radius: 2px;
  margin-right: 10rpx;
}

.comese .colour1 {
  background: #1ccb89;
}

.comese .colour2 {
  background: #5badff;
}

.comese .colour3 {
  background: #fff;
  border: 1px solid #b8b8b8;
}

.comese {
  margin-right: 56rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #505050;
}

.comese-compleet {
  font-size: 20rpx;
  color: #818181;
  margin-right: 10rpx;
}

.increase {
  font-size: 24rpx;
}

.daiban {
  position: absolute;
  right: 28rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  line-height: 42rpx;
  color: #017afd;
}

.daiban .iconfont {
  font-size: 28rpx;
  color: #017afd;
}
