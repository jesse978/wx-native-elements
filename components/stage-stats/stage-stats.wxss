@import "/assets/style/public_icon.wxss";
@import "/app.wxss";

.agenda {
  height: 100%;
  display: flex;
  flex-direction: column;
}

/* 组合 */
.atTopThe {
  background: #fff;
  padding: 16rpx 0;
  margin-bottom: 10rpx;
}

.agenda .onthe {
  padding: 0 28rpx;
  margin-top: 8rpx;
}

.louzu-tag {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.louzu-left {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-weight: bold;
  color: #646464;
}

.tag-building {
  color: #78b8fe;
  margin-right: 12rpx;
  font-weight: normal;
  vertical-align: middle;
  font-size: 30rpx;
}

.fag-building {
  color: #78b8fe;
  margin-right: 12rpx;
  font-weight: normal;
  vertical-align: middle;
  font-size: 28rpx;
}

.louzu-right {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.louzu-cont {
  position: relative;
  display: flex;
  height: 68rpx;
  margin-top: 5rpx;
}

.button {
  display: flex;
  flex-wrap: wrap;
}

.louzu-but {
  max-width: 450rpx;
  min-width: 136rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 20rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: #f6f6f6;
  border-radius: 6rpx;
  text-align: center;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.button .but-active {
  color: #fff;
  background: #017afd;
}

.little {
  position: absolute;
  right: 0;
  width: 58rpx;
  height: 58rpx;
  background: #f6f6f6;
  border-radius: 6rpx;
  text-align: center;
  align-self: flex-start;
}

.little-more {
  font-size: 14rpx;
  color: #818181;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.little-add {
  font-size: 20rpx;
  color: #818181;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.gaiyao {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

/* 滚动区域 */
.scroll {
  flex: 1;
  height: 0;
}

.filter-row {
  position: relative;
  display: flex;
  align-items: center;
  background-color: #ffffff;
}

.neri {
  padding-left: 28rpx;
  height: 60rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #646464;
}

.neri .iconfont {
  font-size: 14rpx;
  color: #646464;
  margin-left: 12rpx;
}

/* 产品统计 */
.stat {
  background: #fff;
}

.sunm-tag {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 18rpx 30rpx;
}

.louzu-left {
  font-size: 30rpx;
  font-weight: bold;
  color: #646464;
  display: flex;
  align-items: center;
}

.tag-building {
  font-size: 28rpx;
  color: #78b8fe;
  line-height: 1;
  margin-right: 10rpx;
  font-weight: normal;
}

.louzu-right {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.right-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 58rpx;
  background: #efeeee;
  border-radius: 8rpx;
}

.right-box .state-box {
  width: 134rpx;
  height: 52rpx;
  text-align: center;
  line-height: 52rpx;
  border-radius: 8rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #535353;
}

.right-box .state-box.checked {
  background: #017afd;
  color: #ffffff;
}

/* 统计表格 */
.stat-head {
  padding: 24rpx 0 24rpx 25rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
  font-weight: bold;
  color: #646464;
  background: #f8f8f8;
}

.stat-head text {
  flex: 1;
  text-align: center;
}

.stat-head text:first-child {
  text-align: left;
}

.stat-content {
  display: flex;
  flex-direction: column;
}

.stat-item {
  flex: 1;
  display: flex;
  justify-content: space-between;
  padding: 0 0 0 25rpx;
  border-bottom: 1rpx solid #f2f2f2;
  align-items: center;
}

.statbt {
  flex: 1;
  position: relative;
  line-height: 1;
}

.statbt .iconfont {
  position: absolute;
  left: -25rpx;
  top: 45%;
  font-size: 12rpx;
  color: #646464;
  /* transform: rotate(180deg); */
}

.statbt .iconfont.up {
  transform: rotate(180deg);
}

.header {
  font-size: 26rpx;
  font-weight: bold;
  color: #646464;
}

.describe {
  display: block;
  font-size: 24rpx;
  font-weight: 400;
  color: #646464;
  line-height: 1;
}

.statms {
  min-height: 90rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  font-weight: 400;
  color: #646464;
  flex: 1;
  text-align: center;
}

.statms.border {
  border-left: #efefef 1px solid;
}

.statms.checked {
  color: #017afd;
  font-weight: bold;
}

.statms.checked text {
  width: 84rpx;
  height: 51rpx;
  line-height: 51rpx;
  background: #f8f8f8;
  border-radius: 8rpx;
}

.state-num {
  position: absolute;
  top: 12rpx;
  right: 2rpx;
  line-height: 1;
  color: #888888;
  font-size: 18rpx;
}

.state-num.add {
  color: #34ab16;
}

.state-num.minus {
  color: #d11f1f;
}

/* 房间信息 */
.floor-what {
  display: flex;
  border-top: 2rpx solid #e6e6e6;
}

.floor-sun {
  display: flex;
  justify-content: space-between;
  flex: 1;
}

.floor-nav {
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #e6e6e6;
  background: #fff;
}

.tag-explain {
  width: 24rpx;
  text-align: right;
  margin-right: 28rpx;
}

.ban {
  width: 90rpx;
}

.floor-build {
  height: 100%;
  width: 90rpx;
  border-right: 2rpx solid #e6e6e6;
}

.build {
  height: 109rpx;
  text-align: center;
  font-size: 28rpx;
  color: #818181;
  position: relative;
}

.building {
  width: 100%;
  line-height: 1;
  font-weight: bold;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  border-left: solid 6rpx transparent;
}

.build-active .building {
  color: #017afd;
  border-left: solid 6rpx #017afd;
  line-height: 36rpx;
}

.floor-content {
  display: flex;
  flex-direction: column;
  flex: 1;
  height: 100%;
}

.floor-filter {
  width: 100%;
  display: flex;
  padding: 0 20rpx;
  height: 75rpx;
  align-items: center;
  border-bottom: 2rpx solid#E6E6E6;
}

.filter-left,
.filter-right {
  display: flex;
}

.filter-right {
  position: absolute;
  right: 22rpx;
}

.filter {
  font-size: 30rpx;
  font-weight: 400;
  line-height: 42px;
  color: #646464;
  margin-right: 56rpx;
  display: flex;
  align-items: center;
}

.fil-icon {
  font-size: 16rpx;
  margin-left: 10rpx;
}

.suan-icon {
  font-size: 28rpx;
  margin-right: 10rpx;
}

.filter-right .filter:last-child {
  margin: 0;
}

.floor-info {
  margin-left: 20rpx;
  font-size: 26rpx;
  margin: 15rpx 0 10rpx 20rpx;
  color: #646464;
}

.suite-roll {
  flex: 1;
  height: 0;
  padding: 0 20rpx;
}

.list-row {
  display: flex;
}

.floor-box {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 70rpx;
  height: 90rpx;
  background: #f9f9f9;
  border-radius: 4rpx;
  border: 2rpx solid #e9e9e9;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 42rpx;
  color: #646464;
  letter-spacing: 1rpx;
  position: relative;
}

.room-total {
  margin: 16rpx 28rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 40rpx;
  color: #393939;
}

.room-box {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
}

.room-item {
  position: relative;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  margin: 0 0 8rpx 8rpx;
  width: 130rpx;
  height: 90rpx;
  background: #ffffff;
  border-radius: 4rpx;
  border: 2rpx solid #e9e9e9;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 42rpx;
  color: #646464;
}

.room-exception {
  position: absolute;
  top: 4rpx;
  right: 6rpx;
  color: #c14641;
  font-size: 22rpx;
}

.common-item {
  width: 166rpx;
  height: 90rpx;
}

.room-item .divider {
  margin: 0 15rpx;
  height: 32rpx;
  border-left: 2rpx solid #ffffff;
}

