/* components/install-room/install-room.wxss */
@import "/assets/style/public_icon.wxss";
@import "/app.wxss";
page {
  background: #f8f8f8;
}

.room-status {
  display: flex;
  flex-direction: column;
  height: 100vh;
}

.room-tabs {
  border-bottom: 2rpx solid #e6e6e6;
}

.room-content {
  flex: 1;
  height: 0;
  display: flex;
  flex-direction: column;
}

/* 房态顶部 */
.combine {
  border-bottom: 10rpx solid #f2f2f2;
  padding: 16rpx 0;
  background: #fff;
}

/* 楼组筛选 */
.onthe {
  padding: 0 30rpx;
  padding-top: 8rpx;
}

.onthe-top {
  margin-top: 8rpx;
}

.louzu-tag {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.louzu-left {
  font-size: 30rpx;
  font-weight: bold;
  color: #646464;
  display: flex;
  align-items: center;
  margin-bottom: 4rpx;
}

.tag-building {
  font-size: 28rpx;
  color: #78b8fe;
  line-height: 1;
  margin-right: 10rpx;
  font-weight: normal;
}

.louzu-right {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.louzu-cont {
  position: relative;
  display: flex;
  margin-top: 5rpx;
  height: 58rpx;
}

.right-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 58rpx;
  background: #efeeee;
  border-radius: 8rpx;
}

.right-box .state-box {
  width: 134rpx;
  height: 52rpx;
  text-align: center;
  line-height: 52rpx;
  border-radius: 8rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #535353;
}

.right-box .state-box.checked {
  background: #017afd;
  color: #ffffff;
}

.button {
  display: flex;
  flex-wrap: wrap;
}

.louzu-but {
  max-width: 450rpx;
  min-width: 136rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 20rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: #f6f6f6;
  border-radius: 6rpx;
  text-align: center;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.button .but-active {
  color: #fff;
  background: #017afd;
}

.little {
  position: absolute;
  right: 0;
  width: 58rpx;
  height: 58rpx;
  background: #f6f6f6;
  border-radius: 6rpx;
  text-align: center;
  align-self: flex-start;
}

.little-more {
  font-size: 14rpx;
  color: #818181;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.little-add {
  font-size: 20rpx;
  color: #818181;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.gaiyao {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.screen-top {
  background: #fff;
  padding: 18rpx 30rpx;
}

/* 表格 */
.table {
  width: 750rpx;
  display: flex;
  flex-direction: column;
  overflow: auto;
  background: #ffffff;
  border: #efefef 1px solid;
  padding-bottom: 16rpx;
}

.table .table-head {
  display: flex;
  background: #f8f8f8;
}

.table-head .head-item:nth-child(3) {
  border-right: 2rpx solid #e6e6e6;
}

.table .head-item {
  min-width: 130rpx;
  min-height: 75rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  line-height: 37rpx;
  color: #646464;
  text-overflow: ellipsis;
  background: #f8f8f8;
}

.table-content {
  display: flex;
  flex-direction: column;
}

.table-content .table-row {
  display: flex;
}

.table-row .row-item:nth-child(3) {
  border-right: 2rpx solid #e6e6e6;
}

.table-row .row-item {
  min-width: 130rpx;
  height: 85rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  color: #646464;
  border-bottom: 2rpx solid #eeeeee;
}

.head-item .fil-icon {
  margin-left: 20rpx;
}

.conteg {
  line-height: 1;
}

.headline {
  font-weight: bold;
}

.amount {
  line-height: 1;
  margin-top: 8rpx;
  display: block;
  width: 100%;
}

.head-item.name,
.row-item.name {
  min-width: 170rpx;
  padding-left: 28rpx;
  justify-content: flex-start;
  border-right: 2rpx solid #e6e6e6;
}

.row-item.boder {
  border-right: 2rpx solid #e6e6e6;
}

.head-item.blue,
.row-item.blue {
  font-weight: bold;
  color: #017afd;
}

.row-item.blue .iconfont {
  margin-left: 10rpx;
  font-size: 16rpx;
  color: #017afd;
}

.row-item.checked {
  color: #017afd;
  font-weight: bold;
  background-size: 10%;
}

/* 房态内容 */
.floor {
  display: flex;
}

.floor-sun {
  flex: 1;
  /* display: flex; */
}

.floor-what {
  display: flex;
  justify-content: space-between;
  border-top: 2rpx solid #e6e6e6;
  flex: 1;
  height: 0;
}

.ban {
  width: 88rpx;
}

.floor-build {
  height: 100%;
  width: 88rpx;
  border-right: 2rpx solid #e6e6e6;
}

.build {
  height: 109rpx;
  text-align: center;
  font-size: 28rpx;
  color: #818181;
  position: relative;
}

.building {
  width: 100%;
  line-height: 36rpx;
  position: absolute;
  top: 50%;
  font-weight: bold;
  transform: translateY(-50%);
  border-left: solid 4rpx transparent;
}

.build-active .building {
  color: #017afd;
  border-left: solid 6rpx #017afd;
}

.floor-content {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.floor-filter {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  height: 75rpx;
  align-items: center;
  border-bottom: 2rpx solid#E6E6E6;
}

.filter-left {
  display: flex;
}

.filter-right {
  position: absolute;
  right: 22rpx;
  font-size: 30rpx;
  font-weight: 400;
  line-height: 42px;
  color: #646464;
  display: flex;
  align-items: center;
}

.filter {
  font-size: 30rpx;
  font-weight: 400;
  line-height: 42px;
  color: #646464;
  margin-right: 56rpx;
  display: flex;
  align-items: center;
}

.filter-update.filter {
  margin-right: 8rpx;
}

.fil-icon {
  font-size: 16rpx;
  margin-left: 10rpx;
}

.suan-icon {
  font-size: 28rpx;
  margin-right: 10rpx;
}

.filter-right .filter:last-child {
  margin: 0;
}

.floor-info {
  margin-left: 20rpx;
  font-size: 26rpx;
  margin: 15rpx 0 10rpx 20rpx;
  color: #646464;
}

.suite-roll {
  flex: 1;
  height: 0;
  padding: 0 20rpx 50rpx 20rpx;
}

.list-row {
  display: flex;
}

.floor-box {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 70rpx;
  height: 90rpx;
  background: #f9f9f9;
  border-radius: 4rpx;
  border: 2rpx solid #e9e9e9;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 42rpx;
  color: #646464;
  letter-spacing: 1rpx;
  position: relative;
}

.room-total {
  margin: 16rpx 28rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 40rpx;
  color: #393939;
}

.room-box {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
}

.room-item {
  position: relative;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  margin: 0 0 8rpx 8rpx;
  width: 130rpx;
  height: 90rpx;
  background: #ffffff;
  border-radius: 4rpx;
  border: 2rpx solid #e9e9e9;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 42rpx;
  color: #646464;
}

.room-exception {
  position: absolute;
  top: 4rpx;
  right: 6rpx;
  color: #c14641;
  font-size: 22rpx;
}

.common-item {
  width: 166rpx;
  height: 90rpx;
}

.room-item .divider {
  margin: 0 15rpx;
  height: 32rpx;
  border-left: 2rpx solid #ffffff;
}

.room-item.status1 {
  background-color: #f37a53;
  color: #fff;
}

.room-item.status2 {
  background-color: #1ccb89;
  color: #fff;
}

.room-item.status3 {
  background-color: #5badff;
  color: #fff;
}

/* 批量问题 */
.batch-problem {
  position: fixed;
  z-index: 100;
  bottom: 100rpx;
  right: 20rpx;
  width: 90rpx;
  height: 90rpx;
  text-align: center;
  background: linear-gradient(134deg, #ffffff 0%, #ffffff 100%);
  box-shadow: 0px 3px 12px rgba(125, 123, 123, 0.16);
  border-radius: 50%;
}

.batch-piLianwt {
  font-size: 46rpx;
  color: #017afd;
  position: absolute;
  top: 54%;
  left: 54%;
  transform: translate(-50%, -50%);
}

.add-group {
  margin-top: -100rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 37rpx;
  color: #bfbfbf;
}
