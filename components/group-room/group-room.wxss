/* components/group-room/group-room.wxss */
@import "/assets/style/public_icon.wxss";
@import "/app.wxss";
.onthe {
  padding: 0 28rpx;
  margin-top: 8rpx;
}

.louzu-tag {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.louzu-left {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-weight: bold;
  color: #646464;
}

.tag-building {
  color: #78b8fe;
  margin-right: 12rpx;
  font-weight: normal;
  vertical-align: middle;
  font-size: 30rpx;
}

.fag-building {
  color: #78b8fe;
  margin-right: 12rpx;
  font-weight: normal;
  vertical-align: middle;
  font-size: 28rpx;
}

.louzu-right {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.louzu-cont {
  position: relative;
  display: flex;
  height: 68rpx;
  margin-top: 5rpx;
}

.button {
  display: flex;
  flex-wrap: wrap;
}

.louzu-but {
  max-width: 450rpx;
  min-width: 136rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 20rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: #f6f6f6;
  border-radius: 6rpx;
  text-align: center;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}

.button .but-active {
  color: #fff;
  background: #017afd;
}

.little {
  position: absolute;
  right: 0;
  width: 58rpx;
  height: 58rpx;
  background: #f6f6f6;
  border-radius: 6rpx;
  text-align: center;
  align-self: flex-start;
}

.little-more {
  font-size: 14rpx;
  color: #818181;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.little-add {
  font-size: 20rpx;
  color: #818181;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.gaiyao {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
}
