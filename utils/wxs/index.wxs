function textOverflow(str, max) {
  var max = max > 0 ? max : 4;
  if (str && str.length > max) {
    return str.substring(0, max - 1) + "...";
  } else {
    return str;
  }
}
module.exports = {
  textOverflow: textOverflow,
};
